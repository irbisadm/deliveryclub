<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/colResizable-1.3.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/jquery.tablesorter.min.js') }}"></script>
