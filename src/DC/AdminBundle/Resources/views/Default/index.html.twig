<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/jquery.embedly.js') }}"></script>
<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/flowplayer-3.2.11.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/flowplayer.controls-3.2.10.js') }}"></script>
<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/flowplayer.playlist-3.2.10.js') }}"></script>
<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/script.js') }}"></script>
{% endblock %}
</body>
</html>