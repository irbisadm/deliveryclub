<!DOCTYPE html>
<html {% block meta_lang %}lang="en"{% endblock %}>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{% block meta_description %}A complete admin panel theme{% endblock %}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="author" content="irbisadm">

    <title>{% block title %}Deliveryclub admin{% endblock %}</title>
    {% block stylesheet %}
    <link rel="stylesheet" href="{{ asset('/bundles/dcadmin/css/bootstrap.css') }}"/>
    <link rel="stylesheet" href="{{ asset('/bundles/dcadmin/css/bootstrap-responsive.css') }}"/>
    <link rel="stylesheet" href="{{ asset('/bundles/dcadmin/css/style.css') }}"/>
    <link rel="stylesheet" href="{{ asset('/bundles/dcadmin/css/plugins.css') }}">
    {% endblock %}

    {% block javascript_head %}
    <script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/jquery.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/jquery.isotope.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/jquery.infinitescroll.min.js') }}"></script>
    {% endblock %}

    <!--[if IE]>
    <script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/modernizr.custom.js') }}"></script>
    <script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/excanvas.js') }}"></script>
    <![endif]-->

    <!--[if IE 8]>
    <script src="http://ie7-js.googlecode.com/svn/version/2.1(beta4)/IE8.js"></script>
    <link href="{{ asset('/bundles/dcadmin/css/ie8.css') }}" rel="stylesheet">
    <![endif]-->

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <!--[if gte IE 9]>
    <style type="text/css">
        .gradient ul li {
            filter: none;
        }
    </style>
    <![endif]-->
</head>

<body>
{% include 'DCAdminBundle:Default:menu.html.twig' %}
{% include 'DCAdminBundle:Default:hightlights.html.twig' %}

<div class="contentainer_wrapper">
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">

                <div class="wrapper">
                    {% include 'DCAdminBundle:Default:sidebar_navigation.html.twig' %}
                    <div class="content_wrapper">

                        <div class="contents">
                        {% block content %}
                        {% endblock %}
                        </div><!-- content end -->

                    </div><!-- content_wrapper end -->
                </div><!-- wrapper end -->
            </div>
        </div>
    </div>
</div><!-- container_wrapper end -->

<div class="footer_wrapper">
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <div class="footer">
                    All right reserved &reg; by <a href="http://deliveryclub.cz">Deliveryclub</a>
                </div><!-- footer end -->
            </div>
        </div>
    </div>
</div><!-- footer_wrapper end -->
{% block javascript_footer %}
<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/bootstrap.js') }}"></script>
<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/jquery.elastic.source.js') }}"></script>
<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/bootstrap-datepicker.js') }}"></script>
