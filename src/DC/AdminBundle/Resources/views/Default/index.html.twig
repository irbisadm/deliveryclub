<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/jshashtable-2.1_src.js') }}"></script>
<script type="text/javascript" src="{{ asset('/bundles/dcadmin/js/jquery.numberformatter-1.2.3.js') }}"></script>
